  do deu=`echo $p | cut  -d "+" -f 1`
     grep -q $deu $installed
     if [ "$?" -eq "0" ]; then
       eng=`echo $p | cut  -d "+" -f 2`
       sudo apt-get -y install $eng
       sudo apt-get remove $deu
     fi
done
rm $installed
