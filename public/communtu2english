#!/bin/bash
installed=`mktemp`
dpkg --get-selections | grep "\binstall" | awk '{print $1}' > $installed
for p in communtu-drucken+communtu-printing communtu-rechtschreibpruefung+communtu-spell-checker communtu-texte-lesen+communtu-reading-texts communtu-textverarbeitung+communtu-word-processing communtu-grafik+communtu-graphic communtu-geoinformationssysteme+communtu-geographic-information-system communtu-umstieg-von-windows+communtu-changing-from-windows communtu-system-werkzeuge+communtu-system-tools communtu-landkarten+communtu-maps communtu-unfreie-software+communtu-unfree-software communtu-spiele+communtu-games communtu-telefonieren--chatten--mailen+communtu-telephoning--chatting--mailing communtu-multimedia+communtu-multimediaaz-multimedia communtu-compilieren+communtu-compiling communtu-brennen+communtu-burning communtu-audio-grundlagen+communtu-audio--basics- communtu-video-grundlagen+communtu-video--basics- communtu-audio-erweitert+communtu-audio--advanced- communtu-grafik-erweitert+communtu-graphic--advanced- communtu-pc-als-tv-und-videorekorder+communtu-pc-as-a-tv-and-vcr communtu-video-erweitert+communtu-video--advanced- communtu-filmemachen+communtu-making-movies communtu-kompressionstools+communtu-tools-for-compressing communtu-entwicklungs-werkzeuge+communtu-development-tools communtu-texte-einscannen+communtu-scanning-texts communtu-videoschnitt+communtu-video-editing communtu-lernsoftware+communtu-educational-software communtu-texlive-full-deutsch+communtu-texlive-full-german communtu-grundlagen-fuer-webdesign+communtu-webdesign--basics- communtu-private-bundle-eee-pc-fuer-schulen+communtu-private-bundle-eee-pc-for-schools communtu-private-bundle-buero+communtu-private-bundle-office communtu-habu-netbook+communtu-tino-s-first-try communtu-private-bundle-tinos-erster-versuch+communtu-private-bundle-google-eart communtu-rennspiele+communtu-racing-games communtu--bersetzen+communtu-translating communtu-mathematik+communtu-mathematics communtu-emulatoren+communtu-emulators communtu-private-bundle-musik-lehren+communtu-private-bundle-teach-music communtu-private-bundle-musik-lernen+communtu-private-bundle-learn-music communtu-lernspiele+communtu-learn-games communtu-private-bundle-installation-hu-berlin-rechner-berlin+communtu-private-bundle-installation-of-the-hu-berlin-computer-berlin communtu-auswertungen-und-statistiken+communtu-analysis-and-statistics communtu-private-bundle-meinemusik+communtu-private-bundle-mymusic communtu-private-bundle-caros-netbook+communtu-private-bundle-minimal-software-f-os-netbook communtu-private-bundle-azd325-s-buendel+communtu-private-bundle-azd325-s-bundle communtu-ruby-on-rails-mit-mysql+communtu-ruby-on-rails-with-mysql communtu-professionell-musik-machen+communtu-make-music-professionally communtu-chemie+communtu-chemistry communtu-grundlagen+communtu-basics communtu-java-programmierumgebung+communtu-java-programming-environment communtu-mindmaps-und-organisationsdiagramme+communtu-mindmaps-and-organigrams communtu-private-bundle-kino+communtu-private-bundle-cinema communtu-private-bundle-neu+communtu-private-bundle-new communtu-ruby-programmierung+communtu-ruby-programming communtu-private-bundle-mein-testbuendel+communtu-private-bundle-my-test-bundle communtu-private-bundle-python-programmierung+communtu-private-bundle-python-programming communtu-private-bundle-testbuendel+communtu-private-bundle-test-bundletimotestbundle communtu-private-bundle-timos-buendel-2+communtu-private-bundle-timo-s-bundle-2 communtu-private-bundle-torstens-pakete+communtu-private-bundle-torsten-s-packages communtu-maltes-installation+communtu-malte-s-installation communtu-private-bundle--buendel091112+communtu-private-bundle-buendel091112 communtu-debian-metapaket-bau+communtu-building-debian-metapackages communtu-private-bundle-janas-buendel+communtu-private-bundle-janas-bundle communtu-private-bundle-timos-spiele+communtu-private-bundle-timos-games communtu-private-bundle-katrins-installation+communtu-private-bundle-ann-katrins-installation communtu-private-bundle-timos-buendel+communtu-private-bundle-timos-bundle communtu-uebersetzen+communtu-translating communtu-private-bundle-internet-grundlagen-linuxtag-demo+communtu-private-bundle-linuxtag-demo communtu-private-bundle-mp3-tagging+communtu-private-bundle-m3-tagging communtu-private-bundle-test-spiele+communtu-private-bundle-test-games communtu-private-bundle-my-dekstop+communtu-private-bundle-my-desktop communtu-private-bundle-timotestbuendel+communtu-private-bundle-timotestbundle communtu-private-bundle-metapackage-isaac-ardilla+communtu-private-bundle-metapackage-isaac-ardila communtu-private-bundle-tills-installation2+communtu-private-bundle-tills-standardinstallation2 communtu-private-bundle-tills-browser+communtu-private-bundle-tills-brower communtu-private-bundle-nach-installation+communtu-private-bundle-post-installation communtu-private-bundle-juttas-buendel+communtu-private-bundle-juttas-bundle communtu-private-bundle-meine-grafik+communtu-private-bundle-my-graphics communtu-private-bundle-tills-neue-installation+communtu-private-bundle-tills-new-installation communtu-grafik+communtu-private-bundle-graphik communtu-private-bundle-musikunterricht+communtu-private-bundle-music-lessons communtu-private-bundle-aligiii1968-s-buendel+communtu-private-bundle-aligiii1968s-bundle communtu-strategie+communtu-strategy communtu-musik+communtu-music communtu-private-bundle-uwe-s-installation+communtu-private-bundle-uwes-installation communtu-private-bundle-musik-unterrichten+communtu-private-bundle-teach-music communtu-private-bundle-meinegrafik+communtu-private-bundle-mygraphics communtu-private-bundle-grafikprogramme+communtu-private-bundle-graphic-art-software communtu-private-bundle-az-netzwerk+communtu-private-bundle-az-network communtu-rollenspiele+communtu-role-play-games communtu-spiele-von-playdeb.net+communtu-games-from-playdeb.net communtu-private-bundle-az-komplett+communtu-private-bundle-az-complete communtu-private-bundle-az-sonstiges+communtu-private-bundle-az-other 
  do deu=`echo $p | cut  -d "+" -f 1`
     grep -q $deu $installed
     if [ "$?" -eq "0" ]; then
       eng=`echo $p | cut  -d "+" -f 2`
       sudo apt-get -y install $eng
       sudo apt-get -y remove $deu
     fi
done
rm $installed
