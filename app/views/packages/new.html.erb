<h1>New packet</h1>

<%= error_messages_for :packet %>

<% form_for(@packet) do |f| %>
  <p>
    <b>Distrib</b><br />
    <%= f.text_field :distrib_id %>
  </p>

  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Version</b><br />
    <%= f.text_field :version %>
  </p>

  <p>
    <b>Source</b><br />
    <%= f.text_field :source %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', packets_path %>
