<h1><%= t(:edit_architecture)%></h1>

<% form_for(@architecture) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit t(:update) %>
  </p>
<% end %>

<%= link_to t(:show), @architecture %> |
<%= link_to t(:back), architectures_path %>
