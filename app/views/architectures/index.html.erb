<h1><%=t(:architectures)%></h1>

<table>

<% for architecture in @architectures %>
  <tr>
    <td><%=h architecture.name %></td>
    <td><%= link_to t(:show), architecture %></td>
    <td><%= link_to t(:edit), edit_architecture_path(architecture) %></td>
    <td><%= link_to t(:delete), architecture, :confirm => t(:are_you_sure), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t(:new_architecture), new_architecture_path %>
